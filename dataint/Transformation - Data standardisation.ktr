        <jsScript_script>//Script here

// Funções de normalização
function normalizePrice(price) {
    if (price != null) {
        // Garantir duas casas decimais
        return parseFloat(price).toFixed(2);
    }
    return price;
}

function titleCase(text) {
    if (text != null) {
        // Converter para "Title Case"
        return text.toLowerCase().replace(/\b([a-z])/g, function (char) {
            return char.toUpperCase();
        });
    }
    return text;
}

function validateImageURL(url) {
    // Validar URL de imagem com extensões padrão
    return url &amp;&amp; /^https?:\/\/.*\.(jpg|jpeg|png|gif)$/i.test(url) ? url : null;
}

// Funções de limpeza
function removeSpecialChars(text) {
    // Remover caracteres especiais indesejados
    return text != null ? text.replace(/[^\w\s]/g, '') : text;
}

function removeExtraSpaces(text) {
    // Remover espaços consecutivos
    return text != null ? text.replace(/\s{2,}/g, ' ') : text;
}

function removeURLs(text) {
    // Remover URLs na descrição
    return text != null ? text.replace(/https?:\/\/[^\s]+/g, '') : text;
}

function removeHTML(text) {
    // Remover tags HTML da descrição
    return text != null ? text.replace(/&lt;[^>]*>/g, '') : text;
}

function removeCurrencySymbols(price) {
    // Remover símbolos de moeda do preço
    return price != null ? price.replace(/[€$R]/g, '') : price;
}

// Aplicação das funções nas colunas
var new_id = id; // Copia o id sem modificação
var new_title = titleCase(removeSpecialChars(title)); // Normaliza e limpa o título
var new_price = removeCurrencySymbols(normalizePrice(price)); // Limpa e normaliza o preço
var new_description = removeExtraSpaces(removeHTML(removeURLs(description))); // Limpa a descrição
var new_image = validateImageURL(image); // Valida a URL da imagem

// Retorno dos valores normalizados e limpos para uso nas próximas etapas do processo
new_id;
new_title;
new_price;
new_description;
new_image;
</jsScript_script>
      </jsScript>
